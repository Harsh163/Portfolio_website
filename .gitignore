# ========================
# Django Ignore Patterns
# ========================

# Bytecode and cache
__pycache__/
*.py[cod]

# Env file and secrets
.env
*.env

# Database (if SQLite)
*.sqlite3

# Media files (user uploads)
media/

# Collected static files (output from collectstatic)
staticfiles/

# VS Code / IDE
.vscode/
.idea/

# Node / frontend temp files (if using)
node_modules/
dist/
build/

# Git backup files (optional)
*.orig
*.bak

# MacOS / Windows metadata (optional)
.DS_Store
Thumbs.db
